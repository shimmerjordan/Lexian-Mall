.b-b:after {
  bottom: 0;
}
.b-t:after {
  top: 0;
}
/* button样式改写 */
uni-button,
button {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
  font-weight: normal;
}
uni-button.no-border:before, uni-button.no-border:after, button.no-border:before, button.no-border:after {
  border: 0;
}
uni-button[type=default],
button[type=default] {
  color: #303133;
}
/* input 样式 */
.input-placeholder {
  color: #999999;
}
.placeholder {
  color: #999999;
}

